        <result property="sjqfmyrnh" column="sjqfmyrnh"/>
    </resultMap>



    <sql id="UserEtfSharesStaticVo">
        select id, account_id,client_id,goal_id,static_date,
        aagf,
        oneabf,
        di,
        onepgf,
        df,
        oneaef,
        asi,
        aff,
        aif,
        onebf,
        edf,
        oneef,
        nctf,
        gof,
        scf,
        sapbf,
        sapdf,
        gif,
        bal,
        bond,
        sdf,
        sif,
        sof,
        sgdif,
        sgtf,
        gdifmyrh,
        glifmyrnh,
        glifmyr,
        wsgqfmyr,
        wsgqfmyrh,
        cf,
        sjqfmyrnh

        from t_user_etf_shares_static
    </sql>

    <select id="queryListByTime" resultMap="UserEtfSharesStaticRes">
        <include refid="UserEtfSharesStaticVo"/>
        <where>
            <if test="@Ognl@isNotEmpty(accountId)">and account_id=#{accountId}</if>
            <if test="@Ognl@isNotEmpty(clientId)">and client_id=#{clientId}</if>
            <if test="@Ognl@isNotEmpty(goalId)">and goal_id=#{goalId}</if>
            <if test="@Ognl@isNotEmpty(startStaticDate)">
                and static_date>=#{startStaticDate}
            </if>
            <if test="@Ognl@isNotEmpty(endStaticDate)">
                and #{endStaticDate}>=static_date
            </if>
            order by  static_date asc;
        </where>
    </select>

    <select id="selectByStaticDate" resultMap="UserEtfSharesStaticRes">
        <include refid="UserEtfSharesStaticVo"/>
        <where>
            <if test="@Ognl@isNotEmpty(accountId)">and account_id=#{accountId}</if>
            <if test="@Ognl@isNotEmpty(clientId)">and client_id=#{clientId}</if>
            <if test="@Ognl@isNotEmpty(goalId)">and goal_id=#{goalId}</if>
            <if test="@Ognl@isNotEmpty(staticDate)">and DATE_FORMAT(static_date,'%Y-%m-%d')=DATE_FORMAT(#{staticDate},'%Y-%m-%d')</if>
        </where>
    </select>





</mapper>