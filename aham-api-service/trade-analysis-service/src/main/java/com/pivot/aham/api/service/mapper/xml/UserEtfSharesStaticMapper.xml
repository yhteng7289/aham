    <resultMap id="UserEtfSharesStaticRes" type="com.pivot.aham.api.service.mapper.model.UserEtfSharesStaticPO">
        <result property="id" column="id"></result>
        <result property="accountId" column="account_id"></result>
        <result property="clientId" column="client_id"></result>
        <result property="goalId" column="goal_id"></result>
        <result property="staticDate" column="static_date"></result>
        <result property="aagf" column="aagf"/>
        <result property="oneabf" column="oneabf"/>
        <result property="di" column="di"/>
        <result property="onepgf" column="onepgf"/>
